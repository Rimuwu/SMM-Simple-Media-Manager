{

    "telegram_executor": {
        "base_class": "tg.main.TelegramExecutor",
        "config": {
            "token": {
                "env": "TG_BOT_TOKEN"
            }
        }
    },

    "vk_executor": {
        "base_class": "vk.main.VKExecutor",
        "config": {
            "group_id": {
                "env": "VK_GROUP_ID"
            },
            "access_token": {
                "env": "VK_ACCESS_TOKEN"
            }
        }
    }

}